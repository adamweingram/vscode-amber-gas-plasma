	"tokenColors": [
		{
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#666600"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#ffcc00",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#ffcc00"
            }
        },
        {
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "storage.type"
            ],
            "settings": {
                "foreground": "#FFBF00",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#ffcc00"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "invalid",
            "settings": {
