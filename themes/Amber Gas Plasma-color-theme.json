		/// --- FROM ORIGINAL EXTENSION ---
		{
			"scope": "comment",
			"settings": {
				"foreground": "#947114",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword, storage",
			"settings": {
				"foreground": "#C38B16",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#FDCC5B",
				"fontStyle": ""
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#FDCC5B",
				"fontStyle": ""
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#FDCC5B",
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#990000"
