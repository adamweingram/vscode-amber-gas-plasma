        {
            "scope": "markup.content",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#ffcc00",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "support.constant",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "support.type",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "support.class",
            "settings": {
                "foreground": "#ffcc00"
            }
        },
        {
            "scope": "support.variable.declaration",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "meta.function-call",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#ffcc00"
            }
        },
        {
            "scope": "meta.function-call.arguments",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "scope": "meta.selection",
            "settings": {
                "background": "#333300"
            }
